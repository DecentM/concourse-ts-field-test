{
  "name": "@decentm/concourse-ts-field-test",
  "version": "0.0.1",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev src/index.ts",
    "build": "tsc",
    "ci:config": "rimraf .ci && ts-node ./ci/generate.ts compile -o .ci -i ci/config/**/*.ts",
    "test": "ava"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--no-warnings",
      "--loader=ts-node/esm"
    ]
  },
  "devDependencies": {
    "@decentm/concourse-ts": "^0.2.8",
    "ava": "^5.1.0",
    "c8": "^7.12.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@types/node": "^18.11.18"
  }
}