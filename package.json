{
  "name": "@decentm/concourse-examples",
  "version": "0.0.1",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev src/index.ts",
    "build": "tsc",
    "ci:config": "ts-node ./ci/generate.ts",
